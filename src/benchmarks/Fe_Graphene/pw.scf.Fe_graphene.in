&CONTROL
  calculation = 'scf'
  restart_mode='from_scratch'
  verbosity='default'
  prefix='Fe_GR'
  nstep=100,
  tstress = .false.
  tprnfor = .true.
  pseudo_dir = './'
  outdir = './tmp/'
  disk_io = "none"
/

&SYSTEM
  ibrav=4
  celldm(1)=36.6047648
  celldm(3)=0.5
  nat= 127
  ntyp=3
  ecutwfc=36.75     ! 500 eV
!  ecutrho=300
  nbnd = 400
  occupations='smearing'
  smearing='gaussian'
  degauss=0.01
  nspin = 2
  starting_magnetization(3)=1.0
/

&ELECTRONS
  mixing_mode = 'plain'
  mixing_beta = 0.3
  conv_thr =  1.0d-8
/

ATOMIC_SPECIES
